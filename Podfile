platform :ios, '9.0'

use_frameworks!

def compulsory_pods
    pod 'Alamofire', '~> 4.0'
    pod 'SwiftyJSON', :git => 'https://github.com/acegreen/SwiftyJSON', :commit => '8971384559780e631b041ac91b16734a396d4c14'
end

target 'Supl' do
    pod 'SwiftSpinner', :git => 'https://github.com/fortmarek/SwiftSpinner'
    compulsory_pods
end

target 'suplSummary' do  
    compulsory_pods
end
